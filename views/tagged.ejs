<!doctype html>
<head>
<%-include partials/head.ejs %>

<!--jQuery -->
<script src="http://code.jquery.com/jquery-latest.js" type="text/javascript"></script>
<script src="https://maps-api-ssl.google.com/maps/api/js?sensor=false" type="text/javascript"></script>


</head>
<body>
<div id="maincontainer">
<%-include partials/header.ejs %>
    <div id="body"> 
      <div id="container">
        
        <div id="row">
            <p><b>Routes tagged with "<%= tag %>"</b></p>
            <div class="twelvecol"><div id="infooutput"></div>     
        </div>
      </div>
    <div class="clearfooter"></div> 
</div>
    <div id="footer"></div>

</div>
</body>
</html>

<script>
$(document).ready(function(){

var maps = <%- maps %>;
createFavListings(maps);

});

function createFavListings(tagarray) {
 for (var i = 0; i < tagarray.length; i++) {
    var url = tagarray[i]._id;
    var title = tagarray[i].title;
    var fullurl = "<a href='/show/" + url + "'>Route</a>";
    
    var title = "<div>" + title + " | " + fullurl + "</div>";
    $('#infooutput').append(title);
    
  };
};

</script>